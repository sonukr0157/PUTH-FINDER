  <script>
    
    $(function() {

$( "#s" ).autocomplete({
    source: function ( request, response ) {
        $.ajax({
            url: "/autocomplete",
            type: 'GET',
            dataType: "json",
            data: {
                s: request.term
            },
            success: function ( data ) {
                response( data );
            }
        });
    },
    select: function ( event, ui ) {
        $( "#s" ).val( ui.item.label );
        return false;
    }
});

});
